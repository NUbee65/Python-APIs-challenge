    "def get_r2_numpy(TLn_x, TLn_y):\n",
    "    TLn_slope, TLn_intercept = np.polyfit(TLn_x, TLn_y, 1)\n",
    "    TLn_r_squared = 1 - (sum((TLn_y - (TLn_slope * TLn_x + TLn_intercept))**2) / ((len(TLn_y) - 1) * np.var(TLn_y, ddof=1)))\n",
    "    print(TLn_r_squared)"
   ]
  }
